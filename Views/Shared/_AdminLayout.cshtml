									  <li><a href="@Url.Action("AddNewUser", "Admin", new{ valetOrCustomer = 4})"><i class="icon-plus-circle2"></i> <span>Add New IT Valet</span></a></li>
									  <li><a href="@Url.Action("ViewUser", "Admin", new{ valetOrCustomer = 4})"><i class="icon-eye"></i> <span>View IT Valets</span></a></li>
									  <li><a href="@Url.Action("UserUnderReviews", "Admin", new{ valetOrCustomer = 4})"><i class="icon-checkmark3"></i> <span>IT Valet Under Review</span></a></li>
									</ul>
								  </li>
								  <li>
									<a href="@Url.Action("FeedbackRecord", "Admin")"><i class="icon-comments"></i> <span>View Feedbacks</span></a>
								  </li>
								  <li>
									<a href="@Url.Action("UserSubscriptionRecord", "Admin")"><i class="icon-file-text2"></i> <span>View Packages</span></a>
								  </li>
							</ul>
