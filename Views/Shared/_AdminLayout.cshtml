							  <!-- Main -->
								  <li class="navigation-header"><span>Main</span> <i class="icon-menu" title="Main pages"></i></li>
								  <li><a href="ControllerName/HomeActionMethod"><i class="icon-home"></i> <span>Home</span></a></li>
								  <li class="active"><a href="#"><i class="icon-home4"></i> <span>Dashboard</span></a></li>
								  <li>
									<a href="#"><i class="icon-users"></i> <span>Manage Customers</span></a>
									<ul>
									  <li><a href="@Url.Action("AddNewUser", "Admin", new{ valetOrCustomer = 3})"><i class="icon-plus-circle2"></i> <span>Add New Customer</span></a></li>
									  <li><a href="@Url.Action("ViewUser", "Admin", new{ valetOrCustomer = 3})"><i class="icon-eye"></i> <span>View Customers</span></a></li>
									  <li><a href="@Url.Action("UserUnderReviews", "Admin", new{ valetOrCustomer = 3})"><i class="icon-checkmark3"></i> <span>Customers Under Review</span></a></li>
									</ul>
								  </li>
								  <li>
									<a href="#"><i class="icon-users"></i> <span>Manage IT Valets</span></a>
